{
  "type": "object",
  "properties": {
    "name": {
      "type": "string"
    },
    "age": {
      "type": "integer",
      "minimum": 0,
      "maximum": 200
    },
    "class": {
      "type": "string",
      "enum": ["1","3","5"]
    }
  },
  "allOf": [
    {
      "if": [{
        "ipIn": {
          "client_ip": ["1.1.1.1"]
        },
        "neq": {
          "class": null
        }
      },{
        "neq": {
          "class": ""
        }
      }],
      "then":[
        {
          "setExpr": {
            "class:${class}_name:${name}.age":"${age}"
          },
          "set": {


          }
        }
      ]
    }
  ]
}